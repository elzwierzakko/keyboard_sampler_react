index.js",[],["62","63"],"E:\\Code\\Coders_Lab\\Project\\keyboard_sampler_react\\src\\Buttons.js",["64","65"],"E:\\Code\\Coders_Lab\\Project\\keyboard_sampler_react\\src\\SamplePlayer.js",["66","67"],"import React, { Component, useState } from \"react\";\r\nimport './App.css'\r\n\r\n//below is the sampler engine sript. This took quite a bit of time to set up. Take care to not screw it up!\r\n//This setup is for preset 1.\r\n\r\nconst URL = \"https://s3.amazonaws.com/freecodecamp/drums/\";\r\nconst SOUNDFILE_NAMES_AND_KEYS = {\r\n  Q : \"Heater-1\",\r\n  W : \"Kick_n_Hat\",\r\n  E : \"Chord_1\",\r\n  A : \"Heater-6\",\r\n  S : \"punchy_kick_1\", \r\n  D : \"Brk_Snr\",\r\n  Z : \"Dry_Ohh\",\r\n  X : \"Heater-3\",\r\n  C : \"Dsc_Oh\"\r\n}\r\n\r\n// This is to pass the last test.\r\nconst KEYCODES = {\r\n  81 : 'Q',\r\n  87 : 'W',\r\n  69 : 'E',\r\n  65 : 'A',\r\n  83 : 'S',\r\n  68 : 'D',\r\n  90 : 'Z',\r\n  88 : 'X',\r\n  67 : 'C'\r\n};\r\n\r\nclass SamplePlayer extends React.Component {\r\n  constructor(props){\r\n    super();\r\n    this.state = { \r\n      currentSoundId: ''\r\n    };\r\n    this.handleClickPlay = this.handleClickPlay.bind(this);\r\n    this.handleKeyPress = this.handleKeyPress.bind(this);\r\n    this.playSound = this.playSound.bind(this);\r\n  }\r\n  \r\n componentDidMount() {\r\n   document.addEventListener(\"keydown\", this.handleKeyPress);\r\n  }\r\n  \r\n  componentWillUnmount() {\r\ndocument.removeEventListener(\"keydown\", this.handleKeyPress);\r\n  }\r\n  \r\n  playSound(){\r\n    document.getElementById(this.state.currentSoundId).play();\r\n  }\r\n\r\n  handleKeyPress(event){\r\n    \r\n    const key = KEYCODES[event.keyCode]; // this is to pass the last test.\r\n    const isValidKey = Object.keys(SOUNDFILE_NAMES_AND_KEYS).includes(key);\r\n    this.setState( { currentSoundId: (isValidKey)? key : '' } );\r\n  }\r\n      \r\n  handleClickPlay(event){\r\n    this.setState( { currentSoundId: event.target.children[0].id } );\r\n  }\r\n  \r\n  render() {\r\n    let message = 'No sound!';\r\n    if (this.state.currentSoundId) {\r\n      message = `Sound: ${SOUNDFILE_NAMES_AND_KEYS[this.state.currentSoundId]}`;\r\n      this.playSound();  \r\n    }\r\n    \r\n    return (\r\n      <div id=\"sample-listing\">\r\n        <Display currentSoundText={message}/>\r\n        <div id=\"sample-icons\">\r\n          {\r\n            Object.keys(SOUNDFILE_NAMES_AND_KEYS).map((key) => {\r\n              return (\r\n                <SamplerMachine\r\n                  name={key} \r\n                  soundFile={URL + SOUNDFILE_NAMES_AND_KEYS[key] + \".mp3\"}\r\n                  clickplayer={this.handleClickPlay}\r\n                  key={key}\r\n                />\r\n              )\r\n            })\r\n          }\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nfunction Display(props){\r\n    return (\r\n    <div id=\"display\">\r\n      {props.currentSoundText}\r\n    </div>\r\n  )\r\n}\r\n\r\nfunction SamplerMachine(props) {\r\n  return (\r\n    <li className=\"sample-icon\" id={props.soundFile} onClick={props.clickplayer}>\r\n      {props.name}\r\n      <audio className=\"clip\" id={props.name} src={props.soundFile} type=\"audio/mp3\" ></audio>\r\n    </li>\r\n  )\r\n}\r\n\r\nexport default SamplePlayer","E:\\Code\\Coders_Lab\\Project\\keyboard_sampler_react\\src\\main.js",["68","69","70","71"],"E:\\Code\\Coders_Lab\\Project\\keyboard_sampler_react\\src\\preset2.js",["72","73","74","75"],"E:\\Code\\Coders_Lab\\Project\\keyboard_sampler_react\\src\\preset3.js",["76","77","78","79"],"E:\\Code\\Coders_Lab\\Project\\keyboard_sampler_react\\src\\buttons.js",["80","81","82","83","84","85","86","87","88"],"import React, { Component, useState } from \"react\";\r\nimport './App.css';\r\nimport SamplePlayer from \"./SamplePlayer\"\r\nimport {\r\n    BrowserRouter as Router,\r\n    Switch,\r\n    Route,\r\n    Link\r\n  } from \"react-router-dom\";\r\nimport Main from \"./main\"\r\nimport Preset2 from \"./preset2\"\r\nimport Preset3 from \"./preset3\"\r\n\r\n\r\nconst Buttons = () => {\r\n    const [infoState, infoChange] = useState(false);\r\n    const toggleInfo = () => {\r\n      if (infoState === true) {\r\n        infoChange(false);\r\n      } else {\r\n        infoChange(true);\r\n      }\r\n    };\r\n  \r\n    return (\r\n      <>\r\n        <div className=\"youtube-info-container\">\r\n\r\n            <div className=\"app-info\" onClick={toggleInfo}>\r\n            </div>\r\n\r\n            <div className=\"change-preset\"> \r\n            <Link to=\"/\">PRESET 1</Link>\r\n            </div>\r\n            <div className=\"change-preset\"> \r\n            <Link to=\"/preset2\">PRESET 2</Link>\r\n            </div>\r\n            <div className=\"change-preset\"> \r\n            <Link to=\"/preset3\">PRESET 3</Link>\r\n            </div>\r\n            </div>\r\n        \r\n      \r\n        {/* end of menu element */}\r\n  \r\n        {/* below will display content depending on what element will be clicked. */}\r\n  \r\n        <div\r\n          className={`${infoState === false ? \"details_container\" : \"no_display\"}`}\r\n        >        \r\n        {/*below is default content. Will be displayed at start.*/}\r\n        \r\n            <h1>PRESS THE MAPPED KEYS TO PLAY SAMPLES</h1>\r\n            \r\n            <p class=\"info\">\r\n                <ul>\r\n                Clicking on the Info image will display APP information and video tutorial.<div class=\"space\"></div>\r\n                </ul> \r\n                <ul>Hit the Reload Icon to change the current keyboard layout to another preset.\r\n                </ul>\r\n            </p>\r\n            <p class=\"info\">The sampler will display the sample name once a mapped key is pressed. Otherwise \"No Sample\" message will be displayed.</p>\r\n            \r\n            <div id=\"color\" class=\"color\">    \r\n            </div>\r\n        \r\n        </div>\r\n        \r\n        {/* <!-- page info below --> */}\r\n  \r\n        <div\r\n          className={`${infoState === true ? \"page_info_container\" : \"no_display\"}`}\r\n        >\r\n          <h1>APP INFO</h1>\r\n          <p class=\"info\">\r\n        Zwierzaks keyboard sampler was developed by Arkadiusz Smolińśki for the sole purpose of completing\r\n        a FRONT END REACT developer course at CODERS LAB. <div class=\"space\"></div>\r\n        If however you by some chance come across this application,\r\n        feel free to use it as you see fit.<div class=\"space\"></div>\r\n        It is possible to use the tool as footswith connected to a bluetooth column for live preformances. <div class=\"space\"></div>\r\n        To change bettween pressets press the arrow icon seen to far right of the APP.<div class=\"space\"></div>\r\n        If you have any questions or feedback, feel free to contact me at:<div class=\"space\"></div>\r\n\r\n        <div>\r\n            <p class=\"info\">\r\n                          \r\n              <form\r\n                action=\"mailto:el.zwierzakko@gmail.com\"\r\n                method=\"post\"\r\n                encType=\"text/plain\"\r\n              >\r\n                <input\r\n                  type=\"submit\"\r\n                  className=\"send button\"\r\n                  value=\"el.zwierzakko at gmail.com\"\r\n                ></input>\r\n              </form>\r\n\r\n            </p>\r\n          </div>\r\n\r\n        </p>\r\n        <p class=\"info\">\r\n        Below you will find a video tutorial on the features and potential uses of the sampler.\r\n        \r\n        </p>\r\n\r\n        <div class=\"info video\">\r\n        \r\n        <iframe width=\"80%\" height=\"315\" src=\"https://www.youtube.com/embed/dQw4w9WgXcQ\" frameborder=\"0\" allow=\"accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture\" allowfullscreen></iframe>\r\n\r\n        </div>\r\n\r\n            \r\n        </div>\r\n      </>\r\n    );\r\n    \r\n};\r\n\r\nexport default Buttons\r\n\r\n\r\n// <div class=\"youtube\">\r\n// </div>\r\n\r\n// <div class=\"app-info\">\r\n// </div>\r\n\r\n\r\n// <div class=\"change-preset\">\r\n// </div>\r\n\r\n// </div>\r\n// <Buttons></Buttons>\r\n// </div>","E:\\Code\\Coders_Lab\\Project\\keyboard_sampler_react\\src\\SamplePlayer2.js",[],["89","90"],"E:\\Code\\Coders_Lab\\Project\\keyboard_sampler_react\\src\\SamplePlayer3.js",["91","92"],"E:\\Code\\Coders_Lab\\Project\\keyboard_sampler_react\\src\\Main.js",["93"],"E:\\Code\\Coders_Lab\\Project\\keyboard_sampler_react\\src\\PageInfo.js",["94"],"E:\\Code\\Coders_Lab\\Project\\keyboard_sampler_react\\src\\Preset2.js",["95"],"E:\\Code\\Coders_Lab\\Project\\keyboard_sampler_react\\src\\Preset3.js",["96"],{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","replacedBy":"100"},{"ruleId":"101","severity":1,"message":"102","line":1,"column":17,"nodeType":"103","messageId":"104","endLine":1,"endColumn":26},{"ruleId":"101","severity":1,"message":"105","line":3,"column":27,"nodeType":"103","messageId":"104","endLine":3,"endColumn":33},{"ruleId":"101","severity":1,"message":"102","line":1,"column":17,"nodeType":"103","messageId":"104","endLine":1,"endColumn":26},{"ruleId":"101","severity":1,"message":"106","line":1,"column":28,"nodeType":"103","messageId":"104","endLine":1,"endColumn":36},{"ruleId":"101","severity":1,"message":"105","line":6,"column":22,"nodeType":"103","messageId":"104","endLine":6,"endColumn":28},{"ruleId":"101","severity":1,"message":"107","line":7,"column":5,"nodeType":"103","messageId":"104","endLine":7,"endColumn":11},{"ruleId":"101","severity":1,"message":"108","line":8,"column":5,"nodeType":"103","messageId":"104","endLine":8,"endColumn":10},{"ruleId":"101","severity":1,"message":"109","line":9,"column":5,"nodeType":"103","messageId":"104","endLine":9,"endColumn":9},{"ruleId":"101","severity":1,"message":"105","line":6,"column":22,"nodeType":"103","messageId":"104","endLine":6,"endColumn":28},{"ruleId":"101","severi